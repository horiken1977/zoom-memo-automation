{
  "version": 2,
  "name": "zoom-memo-automation",
  "builds": [
    {
      "src": "1.src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/0.docs/index.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/0.docs/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/1.src/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/0.docs/index.html"
    }
  ],
  "headers": [
    {
      "source": "/0.docs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/0.docs/index.html"
    }
  ],
  "functions": {
    "1.src/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}