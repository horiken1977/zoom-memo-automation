{
  "version": 2,
  "name": "zoom-memo-automation",
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/",
      "dest": "/0.docs/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/0.docs/dashboard.html"
    },
    {
      "src": "/functional-design",
      "dest": "/0.docs/functional-design.html"
    },
    {
      "src": "/environment-design",
      "dest": "/0.docs/environment-design.html"
    },
    {
      "src": "/test-specifications",
      "dest": "/0.docs/test-specifications.html"
    },
    {
      "handle": "filesystem"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "framework": null
}